{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-unused-vars": "off",
        "react/no-array-index-key": "warn",
        "react/jsx-props-no-spreading": "off",
        "react/prop-types": "off",
        "react/jsx-filename-extension": [
            1,
            {
              "extensions": [".js", ".jsx"]
            }
          ],
          "no-param-reassign": ["error", {
            "props": true,
            "ignorePropertyModificationsFor": [
              "state"
            ]
          }]
    }
}
